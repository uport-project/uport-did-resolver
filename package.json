{
  "name": "uport-did-resolver",
  "version": "0.0.5-alpha.1",
  "description": "Resolve uPort DID documents",
  "main": "lib/register.js",
  "types": "lib/register.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/uport-project/uport-did-resolver.git"
  },
  "files": [
    "dist",
    "lib",
    "vendor.d.ts"
  ],
  "author": "Pelle Braendgaard <pelle.braendgaard@consensys.net>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "tsc --watch",
    "format": "prettier"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "did-resolver": "0.0.6",
    "uport-lite": "^1.0.3"
  }
}
